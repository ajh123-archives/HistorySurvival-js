{
  "name": "history-survival",
  "version": "0.0.5",
  "description": "A voxel game engine with multiplayer and planets",
  "type": "module",
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "build:serve": "npm run build && npm run serve",
    "serve": "node ./src/server/index.js",
    "watch": "nodemon --exec \"npm run build && node ./src/server/index.js\" -e js --ignore ./dist/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajh123-development/HistorySurvival.git"
  },
  "keywords": [
    "voxel",
    "game",
    "planets",
    "cities",
    "multiplayer"
  ],
  "author": "ajh123",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ajh123-development/HistorySurvival/issues"
  },
  "homepage": "https://github.com/ajh123-development/HistorySurvival#readme",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.6.1",
    "softxels": "^0.0.57"
  },
  "devDependencies": {
    "@web/rollup-plugin-html": "^1.11.0",
    "nodemon": "^2.0.21",
    "rollup": "^3.19.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  }
}
